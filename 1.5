#include<iostream>
 
#include <iostream>
#include <iomanip>
using namespace std;
int hhc(int n,int a[])
{
	int i,a1=0,a2=0,a3=0,a4=0,a5=0;
	for(i=0;i<n;i++)
	{
		switch(a[i])
		{
			case 1:
				a1++;
				break;
			case 2:
				a2++;
				break;
			case 3:
				a3++;
				break;
			case 4:
				a4++;
				break;
			case 5:
				a5++;
				break;
			default:
				break;
	    }
	}
	cout<<"1:";
	for(i=0;i<a1;i++)cout<<"*";
	cout<<endl;
	cout<<"2:";
	for(i=0;i<a2;i++)cout<<"*";
	cout<<endl;
	cout<<"3:";
	for(i=0;i<a3;i++)cout<<"*";
	cout<<endl;
	cout<<"4:";
	for(i=0;i<a4;i++)cout<<"*";
	cout<<endl;
	cout<<"5:";
	for(i=0;i<a5;i++)cout<<"*";
	cout<<endl;
}
int main()
{
	int m,n,i,j,a[20];
	cin>>m;
	for(i=0;i<m;i++)
	{
		cin>>n;
		for(j=0;j<n;j++)
		{
			cin>>a[j];
		}
		hhc(n,a);
	}
	return 0;
}
